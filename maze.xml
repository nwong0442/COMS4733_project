<mujoco model="maze_model">

    <asset>
        <!-- Load your STL mesh -->
        <mesh name="maze" file="MazeIII.STL" />
    </asset>

    <worldbody>
        <body name="maze" pos="0 0 0">
            <!-- Render and collide using the STL -->
            <geom type="mesh" mesh="maze" name="maze_geom" 
                  rgba="0.7 0.7 0.7 1" />
        </body>
    </worldbody>

</mujoco>
